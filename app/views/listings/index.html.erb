<div class="container mt-3">
  <div class="row">
    <div class="col-12">
      <h1>All Listings</h1>
    </div>
  </div>

  <div class="row">
    <div class="col">
     <% @listings.each do | listing | %>
     <%= cl_image_tag listing.photo.key, height: 300, width: 400, crop: :fill %>
     <%= listing.name %>
     <%end%>
    </div>
  </div>
